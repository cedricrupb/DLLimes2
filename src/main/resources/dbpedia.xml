<?xml version="1.0" encoding="UTF-8" ?>
<DLLIMES>
    <PREFIX>
        <NAMESPACE>father</NAMESPACE>
        <LABEL>http://example.com/father#</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>rdf</NAMESPACE>
        <LABEL>http://www.w3.org/1999/02/22-rdf-syntax-ns#</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>rdfs</NAMESPACE>
        <LABEL>http://www.w3.org/2000/01/rdf-schema#</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>foaf</NAMESPACE>
        <LABEL>http://xmlns.com/foaf/0.1/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>dboPopulate</NAMESPACE>
        <LABEL>http://dbpedia.org/ontology/PopulatedPlace/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>dbo</NAMESPACE>
        <LABEL>http://dbpedia.org/ontology/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>dbr</NAMESPACE>
        <LABEL>http://dbpedia.org/resource/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>dbw</NAMESPACE>
        <LABEL>http://wikidata.dbpedia.org/resource/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>dbp</NAMESPACE>
        <LABEL>http://dbpedia.org/property/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>geo</NAMESPACE>
        <LABEL>http://www.w3.org/2003/01/geo/wgs84_pos#</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>georss</NAMESPACE>
        <LABEL>http://www.georss.org/georss/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>purl</NAMESPACE>
        <LABEL>http://purl.org/dc/terms/</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>owl</NAMESPACE>
        <LABEL>http://www.w3.org/2002/07/owl#</LABEL>
    </PREFIX>

    <SOURCE>
        <ID>dbp</ID>
        <ENDPOINT>http://live.dbpedia.org/sparql</ENDPOINT>
        <VAR>?x</VAR>
        <PAGESIZE>1000</PAGESIZE>
        <GRAPH>http://dbpedia.org</GRAPH>
        <RESTRICTION>?x a dbo:City</RESTRICTION>
        <TYPE>sparql</TYPE>
    </SOURCE>
    <TARGET>
        <ID>dbw</ID>
        <ENDPOINT>http://live.dbpedia.org/sparql</ENDPOINT>
        <VAR>?y</VAR>
        <PAGESIZE>1000</PAGESIZE>
        <GRAPH>http://dbpedia.org</GRAPH>
        <TYPE>sparql</TYPE>
    </TARGET>

    <MAPPING>
        <POSITIVE>
            <SOURCE>dbr:Berlin</SOURCE>
            <TARGET>dbr:Berlin</TARGET>
        </POSITIVE>
        <POSITIVE>
            <SOURCE>dbr:Prague</SOURCE>
            <TARGET>dbr:Prague</TARGET>
        </POSITIVE>
        <POSITIVE>
            <SOURCE>dbr:London</SOURCE>
            <TARGET>dbr:London</TARGET>
        </POSITIVE>
        <POSITIVE>
            <SOURCE>dbr:Rome</SOURCE>
            <TARGET>dbr:Rome</TARGET>
        </POSITIVE>
        <POSITIVE>
            <SOURCE>dbr:Moscow</SOURCE>
            <TARGET>dbr:Moscow</TARGET>
        </POSITIVE>
        <POSITIVE>
            <SOURCE>dbr:Paris</SOURCE>
            <TARGET>dbr:Paris</TARGET>
        </POSITIVE>
        <NEGATIVE>
            <SOURCE>dbr:Berlin</SOURCE>
            <TARGET>dbw:Paris</TARGET>
        </NEGATIVE>
        <NEGATIVE>
            <SOURCE>dbr:Moscow</SOURCE>
            <TARGET>dbr:London</TARGET>
        </NEGATIVE>
        <NEGATIVE>
            <SOURCE>dbr:Rome</SOURCE>
            <TARGET>dbr:Prague</TARGET>
        </NEGATIVE>
        <NEGATIVE>
            <SOURCE>dbr:Rome</SOURCE>
            <TARGET>dbr:Paris</TARGET>
        </NEGATIVE>
        <NEGATIVE>
            <SOURCE>dbr:Berlin</SOURCE>
            <TARGET>dbr:Moscow</TARGET>
        </NEGATIVE>
    </MAPPING>

    <MLALGORITHM>
        <NAME>wombat simple</NAME>
        <THRESHOLD>0.9</THRESHOLD>
        <TYPE>unsupervised</TYPE>
        <PARAMETER>
            <NAME>max execution time in minutes</NAME>
            <VALUE>5</VALUE>
        </PARAMETER>
        <PARAMETER>
            <NAME>property_min_f_score</NAME>
            <VALUE>0.6</VALUE>
        </PARAMETER>
        <PARAMETER>
            <NAME>property_min_coverage</NAME>
            <VALUE>0.9</VALUE>
        </PARAMETER>
    </MLALGORITHM>


    <TERMINATE>
        <ITERATION>2</ITERATION>
        <FIXPOINT>TRUE</FIXPOINT>
        <FILE>/Users/cedricrichter/Documents/DL-LIMES/DLLimes/src/main/resources/config/same_person.nt</FILE>
    </TERMINATE>
</DLLIMES>